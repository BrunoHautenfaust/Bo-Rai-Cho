<?xml version="1.0" encoding="utf-8" ?> 
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-us" Name="WinAppCommandSet_en-us">
    <CommandPrefix> App, </CommandPrefix>
    <Example> pick </Example>

    <Command Name="pickColor">
      <Example> {color} </Example>
      <ListenFor> {color} </ListenFor>
      <Feedback> {color} </Feedback>
      <Navigate/>
    </Command>

    <Command Name="clearCanvas">
      <Example> Clear </Example>
      <ListenFor> Clear </ListenFor>
      <Feedback> canvas cleared </Feedback>
      <Navigate/>
    </Command>
    
    <Command Name="open">
      <Example> Open </Example>
      <ListenFor> Open </ListenFor>
      <ListenFor> Start </ListenFor>
      <Feedback> opening App </Feedback>
      <Navigate/>
    </Command>
    
    <PhraseList Label="color">
      <Item> Black </Item>
      <Item> Red </Item>
      <Item> Orange </Item>
      <Item> Yellow </Item>
      <Item> Green </Item>
      <Item> White </Item>
      <Item> Gray </Item>
      <Item> Blue </Item>
      <Item> Purple </Item>
      <Item> Brown </Item>
    </PhraseList>

  </CommandSet>
    
<!-- Other CommandSets for other languages -->

</VoiceCommands>
